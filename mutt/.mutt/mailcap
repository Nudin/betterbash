image/*; display
application/pdf; mupdf %s; test=test -n "$DISPLAY" ; nametemplate=%s.pdf
application/x-pdf; mupdf %s; test=test -n "$DISPLAY" ; nametemplate=%s.pdf
audio/*; mpv
video/*; mpv
text/html; /usr/bin/firefox -private-window %s & x-neomutt-keep; test=test -n "$DISPLAY"; nametemplate=%s.html
text/html; w3m -I %{charset} -T text/html; copiousoutput;
